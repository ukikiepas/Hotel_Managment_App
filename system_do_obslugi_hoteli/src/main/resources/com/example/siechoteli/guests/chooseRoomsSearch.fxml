<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="324.0" prefWidth="506.0" style="-fx-background-color: GhostWhite;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.siechoteli.klient.guests.ChooseRoomsSearch">
   <children>
      <ChoiceBox fx:id="hotelNameChoiceBox" layoutX="210.0" layoutY="140.0" prefWidth="150.0" value="Hotel" />
      <ChoiceBox fx:id="availabilityChoiceBox" layoutX="210.0" layoutY="187.0" prefWidth="150.0" value="Availability" />
      <Text fill="#00ffdc" layoutY="65.0" stroke="#2aa775" strokeType="OUTSIDE" strokeWidth="2.0" text="Choose proper room list" textAlignment="CENTER" wrappingWidth="600.0">
         <font>
            <Font name="Lucida Fax Demibold" size="35.0" />
         </font>
      </Text>
      <Button fx:id="sendButton" layoutX="218.0" layoutY="239.0" mnemonicParsing="false" onAction="#search" style="-fx-background-color: GhostWhite;" text="Search" textAlignment="CENTER">
         <graphic>
            <ImageView fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/search.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
